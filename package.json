{
  "name": "algorithm-practice",
  "version": "0.1.9",
  "private": true,
  "main": "electron-main.js",
  "description": "离线算法练习应用 - 基于 WASM 的浏览器端代码执行，支持 JavaScript、TypeScript 和 Python",
  "author": {
    "name": "zxypro1",
    "url": "https://github.com/zxypro1"
  },
  "license": "MIT",
  "homepage": "https://github.com/zxypro1/OfflineLeetPractice",
  "repository": {
    "type": "git",
    "url": "https://github.com/zxypro1/OfflineLeetPractice.git"
  },
  "keywords": [
    "algorithm",
    "code-practice",
    "programming",
    "wasm",
    "electron",
    "offline"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy-local": "npm run build && npm start",
    "setup": "npm install && npm run build",
    "test": "node scripts/test-runner.js",
    "test:watch": "jest --watch",
    "test:coverage": "node scripts/test-runner.js all --coverage",
    "test:api": "jest tests/api",
    "test:specific": "node scripts/test-runner.js specific",
    "test:solutions": "node scripts/test-runner.js specific solution",
    "test:data": "node scripts/test-runner.js specific data",
    "icons:generate": "node scripts/generate-icons.js",
    "electron:dev": "npm run build && electron .",
    "electron:start": "electron .",
    "electron:build": "npm run build && electron-builder --config electron-builder.config.js",
    "electron:build:mac": "npm run build && electron-builder --config electron-builder.config.js --mac",
    "electron:build:win": "npm run build && electron-builder --config electron-builder.config.js --win",
    "electron:build:linux": "npm run build && electron-builder --config electron-builder.config.js --linux",
    "electron:build:all": "npm run build && electron-builder --config electron-builder.config.js --mac --win --linux",
    "electron:pack": "npm run build && electron-builder --config electron-builder.config.js --dir",
    "dist": "npm run electron:build",
    "dist:mac": "npm run electron:build:mac",
    "dist:win": "npm run electron:build:win",
    "dist:linux": "npm run electron:build:linux",
    "dist:all": "npm run electron:build:all",
    "clean": "rm -rf .next dist node_modules/.cache",
    "clean:all": "rm -rf .next dist node_modules",
    "release": "bash scripts/release.sh",
    "release:patch": "bash scripts/release.sh patch",
    "release:minor": "bash scripts/release.sh minor",
    "release:major": "bash scripts/release.sh major",
    "release:dry": "bash scripts/release.sh patch --dry-run"
  },
  "dependencies": {
    "@mantine/core": "7.6.1",
    "@mantine/hooks": "7.6.1",
    "@mantine/notifications": "7.6.1",
    "@monaco-editor/react": "^4.7.0",
    "@tabler/icons-react": "^3.34.1",
    "@types/katex": "^0.16.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "d3-sankey": "^0.12.3",
    "isomorphic-dompurify": "^2.26.0",
    "katex": "^0.16.22",
    "marked": "^16.2.0",
    "mermaid": "^11.10.1",
    "next": "13.5.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.3",
    "recharts": "^3.6.0",
    "@reduxjs/toolkit": "^2.0.1",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "20.4.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/supertest": "^6.0.3",
    "electron": "^31.0.0",
    "electron-builder": "^24.9.1",
    "electron-next": "^3.1.5",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.33.5",
    "supertest": "^6.3.4",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
